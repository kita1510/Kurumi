    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-editor-js": "^2.1.0",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.10.0",
    "react-spinners": "^0.13.8",
